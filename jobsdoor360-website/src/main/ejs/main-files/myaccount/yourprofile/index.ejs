    <!-- body -->
    <main class="main">
      <div class="body_content">
        <section id="about-sc" class="">
          <div class="container">
            <div class="about-cnt">
              <form action="" class="cv-form" id="cv-form">
                <div class="cv-form-blk">
                  <div class="cv-form-row-title">
                    <h3>About section</h3>
                  </div>
                  <div class="cv-form-row cv-form-row-blk">
                    <div class="single-row-data">
                      <div class="form-elem">
                        <label for="firstname" class="form-label"
                          >First Name</label
                        >
                        <input
                          id="firstname"
                          type="text"
                          class="form-control firstname"
                          placeholder="e.g. John"
                          required
                        />
                        <span class="form-text"></span>
                      </div>
                      <div class="form-elem">
                        <label for="middlename" class="form-label"
                          >Middle Name
                          <span class="opt-text">(optional)</span></label
                        >
                        <input
                          id="middlename"
                          type="text"
                          class="form-control middlename"
                          placeholder="e.g. Herbert"
                        />
                        <span class="form-text"></span>
                      </div>
                      <div class="form-elem">
                        <label for="lastname" class="form-label"
                          >Last Name</label
                        >
                        <input
                          id="lastname"
                          type="text"
                          class="form-control lastname"
                          placeholder="e.g. Doe"
                          required
                        />
                        <span class="form-text"></span>
                      </div>
                    </div>

                    <div class="form-elem">
                      <div class="single-row-data">
                        <div class="form-elem">
                          <label for="image" class="form-label"
                            >Your Image</label
                          >
                          <input
                            id="image"
                            type="file"
                            class="form-control image"
                            accept="image/*"
                          />
                        </div>
                        <div class="dropdown">
                          <label for="gender" class="form-label">Gender</label>
                          <select id="gender" name="gender" required>
                            <option value="better not to say">None</option>
                            <option value="male">Male</option>
                            <option value="female">Female</option>
                          </select>
                        </div>
                        <div class="dropdown">
                          <label for="category" class="form-label"
                            >Category</label
                          >
                          <select id="category" name="gender" required>
                            <option value="none">None</option>
                            <option value="General">General</option>
                            <option value="OBC">OBC</option>
                            <option value="ST">ST</option>
                            <option value="SC">SC</option>
                          </select>
                        </div>
                      </div>
                    </div>
                    <div class="form-elem">
                      <label for="address" class="form-label">Address</label>
                      <input
                        id="address"
                        type="text"
                        class="form-control"
                        placeholder="e.g. Lake Street-23"
                        required
                      />
                      <span class="form-text"></span>
                    </div>

                    <div class="single-row-data">
                      <div class="form-elem">
                        <label for="email" class="form-label">Email</label>
                        <input
                          type="email"
                          name="email"
                          id="email"
                          class="form-control"
                          placeholder="e.g. johan@gmail.com"
                          required
                        />
                        <span class="form-text"></span>
                      </div>
                      <div class="form-elem">
                        <label for="phoneno" class="form-label">Phone No</label>
                        <input
                          id="phoneno"
                          type="integer"
                          class="form-control phoneno"
                          placeholder="e.g. 456-768-798, 567.654.002"
                          required
                        />
                        <span class="form-text"></span>
                      </div>
                    </div>

                    <div class="single-row-data">
                      <div class="form-elem">
                        <label for="github" class="form-label">Github</label>
                        <input
                          id="github"
                          type="text"
                          class="form-control email"
                          placeholder="e.g. xyz.github.com"
                          required
                        />
                        <span class="form-text"></span>
                      </div>
                      <div class="form-elem">
                        <label for="cv" class="form-label">Cover Letter</label>
                        <input
                          id="cv"
                          type="file"
                          class="form-control email"
                          accept="cv/*"
                          required
                        />
                        <span class="form-text"></span>
                      </div>
                    </div>
                  </div>
                </div>

                <div class="cv-form-blk">
                  <div class="cv-form-row-title">
                    <h3>Education</h3>
                  </div>

                  <div class="row-separator repeater">
                    <div class="repeater" data-repeater-list="group-education">
                      <div data-repeater-item class="educations_box">
                        <div class="cv-form-row cv-form-row-blk">
                          <div class="form-elem">
                            <label for="" class="form-label">School</label>
                            <input
                              name="school"
                              type="text"
                              class="form-control school"
                              id="school"
                              placeholder="Name of School"
                            />
                            <span class="form-text"></span>
                          </div>
                          <div class="form-elem">
                            <label for="" class="form-label">Degree</label>
                            <input
                              name="edu_degree"
                              type="text"
                              class="form-control edu_degree"
                              id="degree"
                              placeholder="Degree"
                            />
                            <span class="form-text"></span>
                          </div>
                          <div class="single-row-data">
                            <div class="form-elem">
                              <label for="sdate" class="form-label"
                                >Start Date</label
                              >
                              <input
                                name="edu_start_date"
                                type="month"
                                id="sdate"
                                class="form-control edu_start_date"
                                required
                              />
                              <span class="form-text"></span>
                            </div>
                            <div class="form-elem">
                              <label for="edate" class="form-label"
                                >End Date</label
                              >
                              <input
                                name="edu_graduation_date"
                                type="month"
                                id="edate"
                                class="form-control edu_graduation_date"
                                required
                              />
                              <span class="form-text"></span>
                            </div>
                          </div>
                          <div class="single-row-data">
                            <div class="form-elem">
                              <label for="" class="form-label">City</label>
                              <input
                                name="edu_city"
                                type="text"
                                class="form-control edu_city"
                                id="city"
                                placeholder="City Name"
                              />
                              <span class="form-text"></span>
                            </div>
                            <div class="form-elem">
                              <label for="" class="form-label"
                                >Percentage/CGPA</label
                              >
                              <input
                                name="percentage"
                                type="text"
                                id="Percentage"
                                class="form-control edu_description"
                              />
                              <span class="form-text"></span>
                            </div>
                          </div>
                          <div class="remove_button">
                            <button type="button" class="repeater-remove-btn">
                              Remove
                            </button>
                          </div>
                        </div>
                      </div>
                    </div>
                    <button
                      type="button"
                      value="Add"
                      id="addeducation"
                      class="repeater-add-btn"
                    >
                      Add
                    </button>
                  </div>
                </div>

                <div class="cv-form-blk">
                  <div class="cv-form-row-title">
                    <h3>Skills</h3>
                  </div>

                  <div class="row-separator repeater">
                    <div class="repeater" data-repeater-list="group-skill">
                      <div data-repeater-item>
                        <div class="cv-form-row cv-form-row-skills">
                          <div class="form-elem">
                            <input
                              name="skill"
                              type="text"
                              class="form-control skill"
                            />
                            <span class="form-text"></span>
                          </div>
                          <div class="remove_button">
                            <button type="button" class="repeater-remove-btn">
                              Remove
                            </button>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </section>

        <section class="print-btn-sc">
          <div class="container">
            <button type="button" id="btn" class="print-btn btn btn-primary">
              Submit Details
            </button>
          </div>
        </section>

        <!-- jquery cdn -->

        <!-- custom js -->
      </div>
    </main>

